{
  "name": "vscode-ext-t",
  "displayName": "vscode-ext-t",
  "description": "Language support for t",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tlang",
        "aliases": [],
        "extensions": [
          ".t"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tlang",
        "scopeName": "source.t",
        "path": "syntaxes/tlang.tmLanguage.json"
      }
    ]
  }
}
